{
    "name": "hch/elastic-ml-bundle",
    "description": "A Symfony bundle to integrate Elastic Machine Learning.",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Chaabani Hamadi",
            "email": "chaabani.hammadi@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "symfony/framework-bundle": "^5.0 || ^6.0",
        "elasticsearch/elasticsearch": "^7.0 || ^8.0"
    },
    "autoload": {
        "psr-4": {
            "Hch\\ElasticMLBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hch\\ElasticMLBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^11.4"
    }
}
